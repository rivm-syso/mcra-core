<?xml version="1.0" encoding="utf-8"?>
<TableDefinition IsStrongEntity="true">
  <Id>KineticModelDefinitions</Id>
  <Name>PBK model definitions</Name>
  <Description>PBK model definitions.</Description>
  <Aliases>
    <string>PbkModelDefinitions</string>
    <string>PbkModelDefinition</string>
  </Aliases>
  <HiddenAliases>
    <string>RawPbkModelDefinitions</string>
  </HiddenAliases>
  <TargetDataTable>RawPbkModelDefinitions</TargetDataTable>
  <ColumnDefinitions>
    <ColumnDefinition IsPrimaryKey="true" Required="true" FieldType="AlphaNumeric" FieldSize="50">
      <Id>Id</Id>
      <Description>Unique identifier/code of the model.</Description>
      <Aliases>
        <string>id</string>
        <string>idModel</string>
        <string>idPbkModelDefinitions</string>
      </Aliases>
    </ColumnDefinition>
    <ColumnDefinition FieldType="AlphaNumeric" FieldSize="100" IsNameColumn="true">
      <Id>Name</Id>
      <Description>The name of the model.</Description>
      <Aliases>
        <string>Name</string>
        <string>PbkName</string>
      </Aliases>
    </ColumnDefinition>
    <ColumnDefinition FieldType="AlphaNumeric" FieldSize="200">
      <Id>Description</Id>
      <Description>
        Description of the model.
      </Description>
      <Aliases>
        <string>Description</string>
      </Aliases>
    </ColumnDefinition>
    <ColumnDefinition Required="true" FieldType="FileReference" FieldSize="200">
      <Id>FilePath</Id>
      <Description>Path to SBML xml.</Description>
      <Aliases>
        <string>FilePath</string>
      </Aliases>
    </ColumnDefinition>
  </ColumnDefinitions>
</TableDefinition>
