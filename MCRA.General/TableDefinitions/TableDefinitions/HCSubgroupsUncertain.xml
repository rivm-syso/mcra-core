<?xml version="1.0" encoding="utf-8"?>
<TableDefinition>
  <Id>HCSubgroupsUncertain</Id>
  <Name>Hazard characterisations subgroups uncertainty sets</Name>
  <Description>
    Hazard characterisations are dependent on e.g. age and may be uncertain.
  </Description>
  <Aliases>
    <string>HCSubgroupsUncertain</string>
    <string>HCSubgroupUncertain</string>
  </Aliases>
  <HiddenAliases>
    <string>RawHCSubgroupsUncertain</string>
  </HiddenAliases>
  <TargetDataTable>RawHCSubgroupsUncertain</TargetDataTable>
