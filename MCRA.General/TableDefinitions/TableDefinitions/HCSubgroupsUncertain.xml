      <Aliases>
        <string>idUncertaintySet</string>
        <string>idUncertaintySet</string>
        <string>idUncertainty</string>
      </Aliases>
    </ColumnDefinition>
    <ColumnDefinition Required="true" FieldType="AlphaNumeric" FieldSize="50">
      <Id>Value</Id>
      <Description>The uncertain hazard characterisation of the subgroup.</Description>
      <Aliases>
        <string>Value</string>
      </Aliases>
    </ColumnDefinition>
  </ColumnDefinitions>
