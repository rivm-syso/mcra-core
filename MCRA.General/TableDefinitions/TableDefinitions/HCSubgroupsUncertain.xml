      <Id>idSubgroup</Id>
      <Description>The id of the subgroup.</Description>
      <Aliases>
        <string>idSubgroup</string>
      </Aliases>
    </ColumnDefinition>
    <ColumnDefinition Required="true" FieldSize="50" FieldType="AlphaNumeric" OrderRank="1">
