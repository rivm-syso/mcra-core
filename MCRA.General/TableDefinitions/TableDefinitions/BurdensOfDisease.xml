<?xml version="1.0" encoding="utf-8"?>
<TableDefinition>
  <Id>BurdensOfDisease</Id>
  <Name>Burden of disease</Name>
  <Description></Description>
  <Aliases>
    <string>BurdenOfDisease</string>
    <string>BurdensOfDisease</string>
  </Aliases>
  <HiddenAliases>
    <string>RawBurdensOfDisease</string>
    <string>BaselineBodIndicator</string>
    <string>BaselineBodIndicators</string>
  </HiddenAliases>
  <TargetDataTable>RawBurdensOfDisease</TargetDataTable>
  <ColumnDefinitions>
    <ColumnDefinition Required="true" FieldType="AlphaNumeric" FieldSize="50">
      <Id>Population</Id>
      <Description>The population linked to this burden of disease.</Description>
      <Aliases>
        <string>Population</string>
      </Aliases>
      <ForeignKeyTables>
        <string>Populations</string>
      </ForeignKeyTables>
    </ColumnDefinition>
    <ColumnDefinition FieldType="AlphaNumeric" FieldSize="50" Required="true">
      <Id>idEffect</Id>
      <Description>Code of the effect linked to this burden of disease.</Description>
      <Aliases>
        <string>idEffect</string>
        <string>EffectId</string>
        <string>Effect</string>
        <string>EffectCode</string>
      </Aliases>
      <ForeignKeyTables>
        <string>Effects</string>
      </ForeignKeyTables>
    </ColumnDefinition>
    <ColumnDefinition Required="true" FieldType="BodIndicator">
      <Id>BodIndicator</Id>
      <Description>
        The burden of disease indicator linked to this burden of disease.
      </Description>
      <Aliases>
        <string>BodIndicator</string>
        <string>BurdenofDiseaseIndicator</string>
      </Aliases>
    </ColumnDefinition>
    <ColumnDefinition Required="true" FieldType="Numeric">
      <Id>Value</Id>
      <Description>
        The value of the burden of disease indicator.
      </Description>
      <Aliases>
        <string>Value</string>
      </Aliases>
    </ColumnDefinition>
    <ColumnDefinition FieldType="BodIndicatorDistributionType">
      <Id>BodIndicatorUncertaintyDistribution</Id>
      <Description>
        Distribution type (Uniform, Triangular or LogNormal).
        If not specified, the burden of disease indicator is assumed to be a constant factor.
      </Description>
      <Aliases>
        <string>BodUncertaintyDistribution</string>
        <string>BodDistribution</string>
      </Aliases>
      <PossibleValues>
        <PossibleValue>Uniform</PossibleValue>
        <PossibleValue>LogNormal</PossibleValue>
        <PossibleValue>Triangular</PossibleValue>
      </PossibleValues>
    </ColumnDefinition>
    <ColumnDefinition FieldType="Numeric">
      <Id>BodIndicatorLower</Id>
      <Name>BodIndicatorLower</Name>
      <Description>
        Burden of disease indicator lower uncertainty bound.
      </Description>
      <Aliases>
        <string>BodIndicatorLower</string>
        <string>BodLower</string>
      </Aliases>
    </ColumnDefinition>
    <ColumnDefinition FieldType="Numeric">
      <Id>BodIndicatorUpper</Id>
      <Name>BodIndicatorUpper</Name>
      <Description>
        Burden of disease indicator upper uncertainty bound.
      </Description>
      <Aliases>
        <string>BodIndicatorUpper</string>
        <string>BodUpper</string>
      </Aliases>
    </ColumnDefinition>
  </ColumnDefinitions>
</TableDefinition>
