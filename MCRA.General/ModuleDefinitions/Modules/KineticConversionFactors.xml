<?xml version="1.0" encoding="utf-8" ?>
<Module>
  <Id>KineticConversionFactors</Id>
  <Name>Kinetic conversion factors</Name>
  <ModuleType>CalculatorModule</ModuleType>
  <CanCompute>true</CanCompute>
  <ActionType>KineticConversionFactors</ActionType>
  <TableGroup>KineticConversionFactors</TableGroup>
  <AllowDefaultData>true</AllowDefaultData>
  <IsAdvancedActionType>true</IsAdvancedActionType>
  <Description>
    Kinetic conversion factors relate exposures or hazard characterisations from one or more external routes (dietary, non-dietary oral, dermal, inhalation) to an internal target or biological matrix. The corresponding exposure is referred to as internal concentration.
  </Description>
  <ShortDescription>
    Kinetic conversion factors convert exposures or hazard characterisations from one or more external routes or compartments to an internal target or biological matrix. The corresponding exposure is referred to as internal concentration.
  </ShortDescription>
  <DataDescription>
    Specify kinetic conversion factors as data.
  </DataDescription>
  <CalculationSettings>
    <CalculationSetting source="Action">ExposureType</CalculationSetting>
    <CalculationSetting source="Substances">MultipleSubstances</CalculationSetting>
    <CalculationSetting source="HazardCharacterisations">ExposureRoutes</CalculationSetting>
    <CalculationSetting source="ActiveSubstances">FilterByAvailableHazardCharacterisation</CalculationSetting>
    <CalculationSetting>KCFSubgroupDependent</CalculationSetting>
    <CalculationSetting>InternalMatrices</CalculationSetting>
    <CalculationSetting>NumberOfPbkModelSimulations</CalculationSetting>
    <CalculationSetting>ExposureRangeMinimum</CalculationSetting>
    <CalculationSetting>ExposureRangeMaximum</CalculationSetting>
    <CalculationSetting>ComputeBetweenInternalTargetConversionFactors</CalculationSetting>
    <CalculationSetting source="PbkModels">NumberOfDays</CalculationSetting>
    <CalculationSetting source="PbkModels">ExposureEventsGenerationMethod</CalculationSetting>
    <CalculationSetting source="PbkModels">NonStationaryPeriod</CalculationSetting>
    <CalculationSetting source="PbkModels">NonStationaryPeriodInYears</CalculationSetting>
    <CalculationSetting source="PbkModels">UseParameterVariability</CalculationSetting>
    <CalculationSetting source="PbkModels">PbkSimulationMethod</CalculationSetting>
    <CalculationSetting source="PbkModels">LifetimeYears</CalculationSetting>
    <CalculationSetting source="PbkModels">BodyWeightCorrected</CalculationSetting>
    <CalculationSetting source="PbkModelSimulations">PbkOutputResolutionTimeUnit</CalculationSetting>
    <CalculationSetting source="PbkModelSimulations">PbkOutputResolutionStepSize</CalculationSetting>
    <CalculationSetting source="Action">RandomSeed</CalculationSetting>
  </CalculationSettings>
  <UncertaintySettings>
    <UncertaintySetting>ResampleKineticConversionFactors</UncertaintySetting>
  </UncertaintySettings>
  <UncertaintySources>
    <UncertaintySource>KineticConversionFactors</UncertaintySource>
  </UncertaintySources>
  <SelectionInputs>
    <Input>ActiveSubstances</Input>
  </SelectionInputs>
  <CalculationInputs>
    <Input>PbkModels</Input>
  </CalculationInputs>
  <Entities>
    <Entity>Substances</Entity>
  </Entities>
</Module>
