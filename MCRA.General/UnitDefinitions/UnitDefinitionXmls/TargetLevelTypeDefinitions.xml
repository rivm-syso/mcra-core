    <Unit>
      <Id>External</Id>
      <Name>External</Name>
      <ShortName>Ext</ShortName>
      <Description>External exposure.</Description>
      <Aliases>
        <Alias>Ext</Alias>
      </Aliases>
    </Unit>
    <Unit>
      <Id>Internal</Id>
      <Name>Internal</Name>
      <ShortName>Int</ShortName>
      <Description>Internal exposure.</Description>
      <Aliases>
        <Alias>Int</Alias>
      </Aliases>
    </Unit>
  </Units>
</UnitDefinition>
