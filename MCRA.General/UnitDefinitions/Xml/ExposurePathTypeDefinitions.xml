<?xml version="1.0" encoding="utf-8"?>
<UnitDefinition>
  <Id>ExposurePathType</Id>
  <Name>Exposure path type</Name>
  <UndefinedValue>-1</UndefinedValue>
  <Description>
    Accepted paths of exposure to chemicals. A path is a combination of a route and the class of exposure sources (i.e., dietary or non-dietary).
  </Description>
  <Units>
    <Unit>
      <Id>Dietary</Id>
      <Name>Dietary exposure</Name>
      <ShortName>Dietary</ShortName>
      <Description>Dietary exposure.</Description>
      <Aliases>
        <Alias>Dietary</Alias>
      </Aliases>
    </Unit>
    <Unit>
      <Id>Oral</Id>
      <Name>Oral exposure</Name>
      <ShortName>Oral</ShortName>
      <Description>Oral exposure (both from dietary and non-dietary sources).</Description>
      <Aliases>
        <Alias>Oral</Alias>
      </Aliases>
    </Unit>
    <Unit>
      <Id>Dermal</Id>
      <Name>Dermal exposure</Name>
      <ShortName>Dermal</ShortName>
      <Description>Dermal exposure.</Description>
      <Aliases>
        <Alias>Dermal</Alias>
      </Aliases>
    </Unit>
    <Unit>
      <Id>Inhalation</Id>
      <Name>Inhalation exposure</Name>
      <ShortName>Inhalation</ShortName>
      <Description>Inhalation exposure.</Description>
      <Aliases>
        <Alias>Inhalation</Alias>
      </Aliases>
    </Unit>
    <Unit>
      <Id>AtTarget</Id>
      <Name>At target</Name>
      <ShortName>At target</ShortName>
      <Description>Exposures directly at the target (organ).</Description>
      <Aliases>
        <Alias>AtTarget</Alias>
      </Aliases>
    </Unit>
  </Units>
</UnitDefinition>
