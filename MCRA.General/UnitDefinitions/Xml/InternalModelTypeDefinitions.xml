<?xml version="1.0" encoding="utf-8"?>
<UnitDefinition>
  <Id>InternalModelType</Id>
  <Name>Internal model type</Name>
  <Description>
    Methods for translating between different (internal/external) exposure targets.
  </Description>
  <Units>
    <Unit>
      <Id>ConversionFactorModel</Id>
      <Name>Use kinetic conversion factors</Name>
      <ShortName>KCFs</ShortName>
      <Description>Use (low-tier) kinetic conversion factor models.</Description>
      <Aliases>
        <Alias>ConversionFactorModel</Alias>
        <Alias>ConversionFactor</Alias>
      </Aliases>
    </Unit>
    <Unit>
      <Id>PBKModel</Id>
      <Name>Use PBK models with fallback on kinetic conversion factors</Name>
      <ShortName>PBK with fallback</ShortName>
      <Description>Use (high-tier) PBK models with fallback on (low-tier) kinetic conversion factors.</Description>
      <Aliases>
        <Alias>PBKModel</Alias>
        <Alias>PBK</Alias>
      </Aliases>
    </Unit>
    <Unit>
      <Id>PBKModelOnly</Id>
      <Name>Use PBK models</Name>
      <ShortName>PBK</ShortName>
      <Description>Use (high-tier) PBK models.</Description>
      <Aliases>
        <Alias>PBKModelOnly</Alias>
        <Alias>PBKOnly</Alias>
      </Aliases>
    </Unit>
  </Units>
</UnitDefinition>
