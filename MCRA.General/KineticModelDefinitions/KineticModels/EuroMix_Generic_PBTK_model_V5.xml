<?xml version="1.0" encoding="utf-8"?>
<KineticModelDefinition>
  <Id>EuroMix_Generic_PBTK_model_V5</Id>
  <IdModel>EuroMix_Generic_PBTK_model</IdModel>
  <Format>DeSolve</Format>
  <Version>V5</Version>
  <Name>EuroMix Generic PBTK model (v5)</Name>
  <ShortName>EuroMix Generic PBTK model (v5)</ShortName>
  <Description>Cosmos version 5 (adapted 9/11/2018)</Description>
  <VersionInfo>Scaled partition coefficients are supplied, e.g. log_aPoor = log(PCPoor/PCFat). Based on this the partition coefficient themselves are calculated e.g. PCPoor = exp(log_aPoor) * PCFat. In case of variability new partition coefficients are calculated as e.g. PCPoor = exp(log_aPoor) * ResampledPCFat. This preserves the correlation.</VersionInfo>
  <Aliases>
    <Alias>EuroMix_Generic_PBTK_model_V5</Alias>
    <Alias>CosmosV4</Alias>
    <Alias>CosmosV5</Alias>
  </Aliases>
  <FileName>EuroMix_Generic_PBTK_model_V5.dll</FileName>
  <Resolution>Hours</Resolution>
  <EvaluationFrequency>1</EvaluationFrequency>
  <IdBodyWeightParameter>BM</IdBodyWeightParameter>
  <IdBodySurfaceAreaParameter>BSA</IdBodySurfaceAreaParameter>
  <Forcings>
    <Forcing>
      <Id>Dietary</Id>
      <Name>Dietary</Name>
      <Description>Dietary exposure</Description>
      <Route>Dietary</Route>
      <Order>0</Order>
      <Unit>mmoles</Unit>
    </Forcing>
    <Forcing>
      <Id>Dermal</Id>
      <Name>Dermal</Name>
      <Description>Dermal exposure</Description>
      <Route>Dermal</Route>
      <Order>1</Order>
      <Unit>mmoles</Unit>
    </Forcing>
    <Forcing>
      <Id>Inhalation</Id>
      <Name>Inhalation</Name>
      <Description>Inhalatory exposure</Description>
      <Route>Inhalation</Route>
      <Order>2</Order>
      <Unit>mmoles</Unit>
    </Forcing>
  </Forcings>
  <States>
    <State>
      <Id>QVen</Id>
      <Description>State venous blood</Description>
      <Order>0</Order>
      <Unit>mmoles</Unit>
    </State>
    <State>
