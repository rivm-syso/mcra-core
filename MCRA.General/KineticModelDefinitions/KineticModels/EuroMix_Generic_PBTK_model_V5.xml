    </Output>
    <Output>
      <Id>CSkin_u</Id>
      <Description>Viable skin, unexposed</Description>
      <BiologicalMatrix>Skin:Viable:Unexposed</BiologicalMatrix>
      <Order>6</Order>
      <Type>Concentration</Type>
      <Unit>mM</Unit>
    </Output>
    <Output>
      <Id>CSkin_e</Id>
      <Description>Viable skin, exposed</Description>
      <BiologicalMatrix>Skin:Viable:Exposed</BiologicalMatrix>
      <Order>7</Order>
      <Type>Concentration</Type>
      <Unit>mM</Unit>
      <ScalingFactors>
        <ScalingFactor>BSA</ScalingFactor>
        <ScalingFactor>Height_vs</ScalingFactor>
        <ScalingFactor>fsA_exposed</ScalingFactor>
      </ScalingFactors>
    </Output>
    <Output>
      <Id>CSkin_sc_u</Id>
      <Description>Skin stratum corneum, unexposed</Description>
      <BiologicalMatrix>Skin:StratumCorneum:Unexposed</BiologicalMatrix>
