      <Order>5</Order>
      <Unit>mmoles</Unit>
    </State>
    <State>
      <Id>QSkin_u</Id>
      <Description>State viable skin, unexposed</Description>
      <Order>6</Order>
      <Unit>mmoles</Unit>
    </State>
    <State>
      <Id>QSkin_e</Id>
      <Description>State viable skin, exposed</Description>
      <Order>7</Order>
      <Unit>mmoles</Unit>
    </State>
    <State>
      <Id>QSkin_sc_u</Id>
      <Description>State skin stratum corneum, unexposed</Description>
