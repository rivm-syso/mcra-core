      <Description>State arterial blood</Description>
      <Order>1</Order>
      <Unit>mmoles</Unit>
    </State>
    <State>
      <Id>QFat</Id>
      <Description>State fat tissues</Description>
      <Order>2</Order>
      <Unit>mmoles</Unit>
    </State>
    <State>
      <Id>QPoor</Id>
      <Description>State muscle tissues</Description>
      <Order>3</Order>
      <Unit>mmoles</Unit>
    </State>
    <State>
      <Id>QRich</Id>
      <Description>State viscera</Description>
      <Order>4</Order>
      <Unit>mmoles</Unit>
    </State>
    <State>
      <Id>QLiver</Id>
