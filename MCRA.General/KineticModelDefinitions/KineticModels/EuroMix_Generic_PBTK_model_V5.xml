      <Unit>mmoles</Unit>
    </State>
    <State>
      <Id>QSkin_sc_e</Id>
      <Description>State skin stratum corneum, exposed</Description>
      <Order>9</Order>
      <Unit>mmoles</Unit>
    </State>
    <State>
      <Id>QGut</Id>
      <Description>State gut</Description>
      <Order>10</Order>
      <Unit>mmoles</Unit>
    </State>
    <State>
      <Id>QMetab</Id>
      <Description>State metabolisme</Description>
      <Order>11</Order>
      <Unit>mmoles</Unit>
    </State>
  </States>
  <Outputs>
    <Output>
      <Id>CVen</Id>
      <Description>Venous blood</Description>
      <BiologicalMatrix>Blood:Venous</BiologicalMatrix>
      <Order>0</Order>
      <Type>Concentration</Type>
      <Unit>mM</Unit>
      <ScalingFactors>
        <ScalingFactor>scVBlood</ScalingFactor>
      </ScalingFactors>
      <MultiplicationFactors>
        <MultiplicationFactor>0.66667</MultiplicationFactor>
      </MultiplicationFactors>
    </Output>
    <Output>
      <Id>CArt</Id>CArt
      <Description>Arterial blood</Description>
      <BiologicalMatrix>Blood:Arterial</BiologicalMatrix>
      <Order>1</Order>
      <Type>Concentration</Type>
      <Unit>mM</Unit>
      <ScalingFactors>
        <ScalingFactor>scVBlood</ScalingFactor>
      </ScalingFactors>
      <MultiplicationFactors>
        <MultiplicationFactor>0.33333</MultiplicationFactor>
      </MultiplicationFactors>
    </Output>
    <Output>
