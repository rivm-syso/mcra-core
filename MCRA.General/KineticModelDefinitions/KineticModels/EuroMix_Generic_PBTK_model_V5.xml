      <Order>4</Order>
      <Type>Concentration</Type>
      <Unit>mM</Unit>
      <ScalingFactors>
        <ScalingFactor>scVRich</ScalingFactor>
      </ScalingFactors>
    </Output>
    <Output>
      <Id>CLiver</Id>
      <Description>Liver</Description>
      <BiologicalMatrix>Liver</BiologicalMatrix>
      <Order>5</Order>
      <Type>Concentration</Type>
      <Unit>mM</Unit>
      <ScalingFactors>
        <ScalingFactor>scVLiver</ScalingFactor>
