      </Settings>
      <ExcludedSettings>
        <Setting id="CodeReferenceSubstance" />
      </ExcludedSettings>
    </ModuleConfiguration>
    <ModuleConfiguration module="SingleValueRisks">
      <Settings>
        <Setting id="SingleValueRiskCalculationMethod">FromIndividualRisks</Setting>
