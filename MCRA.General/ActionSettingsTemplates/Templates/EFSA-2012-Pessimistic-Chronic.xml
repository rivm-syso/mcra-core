<ActionSettingsTemplate>
  <Id>EFSA-2012-Pessimistic-Chronic</Id>
  <IdTier>EfsaPessimisticChronic</IdTier>
  <Name>Retrospective dietary CRA (EFSA 2012) - Chronic / Pessimistic</Name>
  <ShortName>EFSA 2012 Chronic Pessimistic</ShortName>
  <Description>
    Chronic probabilistic exposure assessment using the pessimistic model settings according to the EFSA Guidance 2012.
    Only processing factors &gt; 1 are applied.
  </Description>
  <ActionType>DietaryExposures</ActionType>
  <ActionSettings>
    <ModuleSettings module="DietaryExposures">
      <Settings>
        <Setting id="ExposureType" value="Chronic" readonly="true" />
        <Setting id="TotalDietStudy" value="false" readonly="true" />
        <Setting id="IsSampleBased" value="true" readonly="true" />
        <Setting id="IsSingleSamplePerDay" value="true" readonly="true" />
        <Setting id="IsProcessing" value="true" readonly="true" />
        <Setting id="IsDistribution" value="false" readonly="true" />
        <Setting id="AllowHigherThanOne" value="true" readonly="true" />
        <Setting id="IntakeModelType" value="OIM" readonly="true" />
        <Setting id="FirstModelThenAdd" value="false" readonly="true" />
        <Setting id="CovariateModelling" value="false" readonly="true" />
        <Setting id="IsSurveySampling" value="false" readonly="true" />
        <Setting id="IsPerPerson" value="false" readonly="true" />
      </Settings>
    </ModuleSettings>
    <ModuleSettings module="ConcentrationModels">
      <Settings>
        <Setting id="DefaultConcentrationModel" value="NonDetectSpikeLogNormal" readonly="true" />
        <Setting id="IsFallbackMrl" value="true" readonly="true" />
        <Setting id="RestrictLorImputationToAuthorisedUses" value="false" readonly="true" />
        <Setting id="NonDetectsHandlingMethod" value="ReplaceByLOR" readonly="true" />
        <Setting id="FractionOfLOR" value="1" readonly="true" />
        <Setting id="FractionOfMrl" value="1" readonly="true" />
        <Setting id="IsSampleBased" value="true" readonly="true" />
        <Setting id="ImputeMissingValues" value="true" readonly="true" />
        <Setting id="CorrelateImputedValueWithSamplePotency" value="true" readonly="true" />
        <Setting id="IsParametric" value="false" readonly="true" />
      </Settings>
    </ModuleSettings>
  </ActionSettings>
</ActionSettingsTemplate>
